<Window x:Class="WPF_Log_Program.LogListDisplay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Log_Program"
        mc:Ignorable="d"
        Title="Logs" Height="550" Width="725" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="delete_button" Content="Delete" HorizontalAlignment="Right" Margin="0,0,90,10" VerticalAlignment="Bottom" Width="75" Click="ClickDeleteButton"/>
        <Button x:Name="new_button" Content="New" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="ClickNewButton"/>
        <Button x:Name="edit_button" Content="Edit" HorizontalAlignment="Left" Margin="10,0,10,10" VerticalAlignment="Bottom" Width="75" Click="ClickEditButton"/>
        <ListBox x:Name="listBox" Margin="10,10,10,35" MouseDoubleClick="DoubleClickListBox"/>

    </Grid>
</Window>
