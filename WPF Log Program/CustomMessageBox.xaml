<Window x:Class="WPF_Log_Program.CustomMessageBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Log_Program"
        mc:Ignorable="d"
        Title="Save" Height="129.609" Width="329.412" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="label" Content="Would you like to save?" HorizontalAlignment="Center" Margin="0,0,18,45" VerticalAlignment="Center" Width="265"/>
        <Button x:Name="yes_button" Content="Yes" HorizontalAlignment="Right" Margin="0,0,175,15" VerticalAlignment="Bottom" Width="74" Click="yes_button_Click"/>
        <Button x:Name="no_button" Content="No" HorizontalAlignment="Right" Margin="0,0,95,15" VerticalAlignment="Bottom" Width="75" Click="no_button_Click"/>
        <Button x:Name="cencel_button" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,15,15" VerticalAlignment="Bottom" Width="74" Click="cencel_button_Click"/>


    </Grid>
</Window>
